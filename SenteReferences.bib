	title = {Clarifying different types of portfolio use},
	journal = {Assessment \& Evaluation in Higher Education},
	uuid = {30E79C2E-B308-4CD9-A2EF-1AFA9A861EDE},
	pages = {625-649},
	year = {2003},
	ISSN = {02602938},
	sentelink = {{http://search.ebscohost.com/login.aspx?direct=true&db=anh&AN=11184902&site=ehost-live},EBSCOHost,PermaLink},
